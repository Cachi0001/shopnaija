{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://growsmallbeez.vercel.app",
      "permanent": false
    }
  ],
  "wildcard": [
    {
      "domain": "*.growsmallbeez.vercel.app",
      "value": "/index.html"
    }
  ]
}